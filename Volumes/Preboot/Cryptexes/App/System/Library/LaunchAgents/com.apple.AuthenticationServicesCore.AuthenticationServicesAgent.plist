<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>EnablePressuredExit</key>
	<true/>
	<key>EnableTransactions</key>
	<true/>
	<key>Label</key>
	<string>com.apple.AuthenticationServicesCore.AuthenticationServicesAgent</string>
	<key>LaunchEvents</key>
	<dict>
		<key>com.apple.xpc.activity</key>
		<dict>
			<key>com.apple.AuthenticationServicesCore.AuthenticationServicesAgent.PeriodicMaintenanceActivity</key>
			<dict>
				<key>AllowBattery</key>
				<true/>
				<key>GracePeriod</key>
				<integer>3600</integer>
				<key>Interval</key>
				<integer>86400</integer>
				<key>Priority</key>
				<string>Utility</string>
				<key>RandomInitialDelay</key>
				<integer>300</integer>
				<key>Repeating</key>
				<true/>
				<key>RequiresClassA</key>
				<true/>
				<key>RequiresNetworkConnectivity</key>
				<true/>
			</dict>
		</dict>
	</dict>
	<key>MachServices</key>
	<dict>
		<key>com.apple.AuthenticationServices.AutoFill</key>
		<true/>
		<key>com.apple.AuthenticationServices.CredentialSharingGroups</key>
		<true/>
		<key>com.apple.AuthenticationServicesCore.AuthenticationServicesAgent</key>
		<true/>
	</dict>
	<key>ProcessType</key>
	<string>Adaptive</string>
	<key>Program</key>
	<string>/System/Cryptexes/App/usr/libexec/AuthenticationServicesAgent</string>
</dict>
</plist>
